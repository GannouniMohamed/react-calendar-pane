{
  "name": "react-calendar-pane",
  "description": "React calendar component",
  "main": "src/Calendar.js",
  "version": "0.0.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomkp/react-calendar"
  },
  "author": "tomkp <tom@tomkp.com>",
  "keywords": [
    "react",
    "react-component",
    "calendar",
    "es6"
  ],
  "dependencies": {
    "moment": "^2.9.0",
    "react": "^0.13.1"
  },
  "devDependencies": {
    "babel": "^4.7.16",
    "babel-core": "^4.6.6",
    "babelify": "^5.0.4",
    "browserify": "^9.0.3",
    "coveralls": "^2.11.2",
    "eslint": "^0.21.2",
    "eslint-plugin-react": "^2.3.0",
    "expect.js": "^0.3.1",
    "express": "^4.12.2",
    "mochify": "^2.6.0",
    "mochify-istanbul": "^2.1.1",
    "react-tools": "^0.13.1"
  },
  "scripts": {
    "build": "babel src --out-dir dist",
    "test": "mochify -R spec",
    "test:watch": "mochify -R spec --watch",
