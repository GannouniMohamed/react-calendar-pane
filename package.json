    "demo": "browserify demo/Example.js -t -o demo/bundle.js",
    "demo:publish": "browserify demo/Example.js -t -o demo/bundle.js && surge demo hard-to-find-door.surge.sh"
  },
  "browserify": {
    "transform": [
      [
        "babelify"
      ]
    ]
